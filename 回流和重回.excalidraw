      "id": "zDAutHZepS_Cdi6Us9xb1",
      "type": "text",
      "x": -2628.481173123373,
      "y": -1432.968977860731,
      "width": 1239,
      "height": 56,
      "angle": 0,
      "strokeColor": "#d9480f",
      "backgroundColor": "transparent",
      "fillStyle": "hachure",
      "strokeWidth": 2,
